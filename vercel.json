{
  "version": 2,
  "buildCommand": "npm install && cd apps/shell && npm run build",
  "outputDirectory": "apps/shell/dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
