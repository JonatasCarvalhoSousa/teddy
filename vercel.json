{
  "version": 2,
  "name": "teddy-teste",
  "builds": [
    {
      "src": "apps/shell/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/shell/dist/index.html"
    }
  ],
  "buildCommand": "cd apps/shell && npm install && npm run build",
  "outputDirectory": "apps/shell/dist",
  "installCommand": "npm install",
  "framework": "vite"
}
